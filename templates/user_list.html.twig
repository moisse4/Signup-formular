{% extends "signup.html" %}

{% block content %}
<table border="1" style="width: 80%;">
    <thead>
        <tr>
            <th>ID</th>
            <th>name</th>
            <th>nachname</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        {% for user in users %}
        <tr>
            <td>{{ user.id }}</td>
            <td>{{ user.name }}</td>
            <td>{{ user.nachname }}</td>
            <td>{{ user.email }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}